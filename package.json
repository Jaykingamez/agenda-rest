{
  "name": "@jaykingamez/agenda-rest",
  "version": "1.1.5",
  "description": "Scheduling as a Service",
  "main": "./dist/index.js",
  "jsnext:main": "./src/index.js",
  "scripts": {
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.js --write $PWD'/**/*.js'",
    "dev": "webpack --mode development",
    "build": "rm -rf dist && webpack --mode production",
    "test": "ava ./dist/test.js && npm run format",
    "start": "npm run dev && node cli.js",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "publish": "npm publish",
    "pushregistry": "bash push-registry.sh",
    "postpublish": "npm run pushregistry gcp && npm run pushregistry do"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jaykingamez/agenda-rest.git"
  },
  "files": [
    "cli.js",
    "settings.js",
    "dist"
  ],
  "keywords": [
    "schedule",
    "scheduling",
    "scheduler",
    "webservice",
    "microservice",
    "restful",
    "rest",
    "agenda"
  ],
  "author": "Keyvan Mir Mohammad Sadeghi <keyvan.m.sadeghi@gmail.com>",
  "license": "MIT",
  "bin": {
    "agenda-rest": "cli.js"
  },
  "bugs": {
    "url": "https://github.com/Jaykingamez/agenda-rest/issues"
  },
  "homepage": "https://github.com/Jaykingamez/agenda-rest#README",
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.18.9",
    "ava": "5.1.0",
    "babel-loader": "9.1.2",
    "eslint": "8.31.0",
    "prettier": "2.8.2",
    "prettier-eslint": "15.0.1",
    "prettier-eslint-cli": "7.1.0",
    "supertest": "6.3.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "agenda": "^5.0.0",
    "babel-runtime": "^6.26.0",
    "commander": "^9.5.0",
    "human-interval": "^2.0.1",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "xo": {
    "space": 2,
    "rules": {
      "ava/no-inline-assertions": 0,
      "import/namespace": 0,
      "promise/param-names": 0,
      "promise/prefer-await-to-then": 0,
      "max-params": [
        "error",
        5
      ],
      "max-nested-callbacks": [
        "error",
        5
      ]
    }
  }
}
